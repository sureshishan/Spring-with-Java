package concurrency;

import java.util.ArrayList;
import java.util.List;

public class ConcurrentDemo extends Thread {

	static List list = new ArrayList();
	
	public void run(){
		Thread.sleep(1000);
	}

	public static void main(String[] args) {
		list.add("A");
		list.add("B");
		list.add("C");

		ConcurrentDemo t1 = new ConcurrentDemo();
		t1.start();
		
		
		
	}

}
